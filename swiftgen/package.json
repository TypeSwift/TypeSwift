{
  "name": "swiftgen",
  "version": "0.1.0",
  "description": "Generate typesafe Swift code using input TypeScript code.",
  "main": "dist/swiftgen.js",
  "scripts": {
    "build": "tsc && npm run copy-config && npm run copy-tests",
    "copy-config": "cpx src/config/config.json dist/config/",
    "copy-tests": "cpx tests/**/* dist/tests",
    "start": "node dist/swiftgen.js"
  },
  "keywords": [],
  "author": {
    "name": "Justin Bush",
    "email": "buzsh@users.noreply.github.com",
    "url": "https://github.com/buzsh"
  },
  "homepage": "https://typeswift.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeSwift/TypeSwift.git"
  },
  "license": "MIT",
  "dependencies": {
    "ts-morph": "^22.0.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "cpx": "^1.5.0"
  }
}
